 vector<int> decompressRLElist(vector<int>& nums) {
        vector<int>res;
        for(int i = 0 ; i<nums.size() ; i++){
          if(i%2==0){
            //   int currIdx = i;
              for(int j = 0 ; j<nums[i] ;j++){
                res.push_back(nums[i+1]);
                // cout<<nums[i]<<"-";
              }
          }
        }
        return res;
    }